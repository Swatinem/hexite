{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}/hexite-vscode"],
      "outFiles": ["${workspaceFolder}/hexite-vscode/out/**/*.js"],
      "preLaunchTask": "npm: compile"
    }
  ]
}
